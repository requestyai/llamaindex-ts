{
  "name": "@requesty/llamaindex",
  "description": "Requesty adapter for LlamaIndex.TS - Use, monitor and scale any LLM provider",
  "author": "Requesty <support@requesty.ai>",
  "version": "0.1.1",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "llamaindex",
    "requesty",
    "llm",
    "ai",
    "language-model",
    "tool-calling",
    "structured-outputs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/requestyai/llamaindex-ts.git"
  },
  "bugs": {
    "url": "https://github.com/requestyai/llamaindex-ts/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://requesty.ai",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@llamaindex/core": "^0.6.14",
    "@llamaindex/env": "^0.1.30",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@llamaindex/core": "^0.6.14",
    "@llamaindex/env": "^0.1.30",
  },
  "dependencies": {
    "openai": "^4.102.0"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
